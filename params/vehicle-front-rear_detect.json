{
    "dataset": "vehicle_front_rear",
    "global": {
        "inputs": {
            "image": [360, 640, 3]
        }
    },
    "local": {
        "outputs": {
            "location": [90, 160, 1],
            "scales": [90, 160, 2]
        }
    },
    "batch_size": 32,
    "labels": {
        "location": ["x", "y"],
        "scales": ["width", "height"]
    },
    "shuffle": true,
    "steps": 500,
    "augmentation": "light",
    "preprocess": null
}
