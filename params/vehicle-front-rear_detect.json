{
    "dataset": "vehicle_front_rear",
    "global": {
        "inputs": {
            "image": {
                "type": "image",
                "shape": [360, 640]
            }
        }
    },
    "local": {
        "bbox": {
            "type": "bboxes",
            "shape": [90, 160]
        }
    },
    "batch_size": 32,
    "labels": {
        "location": ["x", "y"],
        "scales": ["width", "height"]
    },
    "shuffle": true,
    "steps": 500,
    "augmentation": "light",
    "preprocess": null
}
