{
    "dataset": "AOLP",
    "inputs": {
        "image": {
            "type": "image",
            "property": "global",
            "size": [255, 255]
        }
    },
    "outputs": {
        "bbox": {
            "type": "bboxes",
            "property": "global"
        }
    },
    "batch_size": 32,
    "labels": {
        "bbox": ["x", "y", "width", "height"]
    },
    "shuffle": true,
    "steps": 500,
    "augmentation": "light",
    "preprocess": null
}
