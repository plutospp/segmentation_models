{
    "dataset": "AOLP",
    "global": {
        "inputs": {
            "image": {
                "type": "image",
                "size": [255, 255]
            }
        },
        "outputs": {
            "bbox": {"type": "bboxes"}
        }
    },
    "batch_size": 32,
    "labels": {
        "bbox": ["x", "y", "width", "height"]
    },
    "shuffle": true,
    "steps": 500,
    "augmentation": "light",
    "preprocess": null
}
