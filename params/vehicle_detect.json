{
    "dataset": "MIOTCD",
    "inputs": {
        "image": [360, 640, 3]
    },
    "outputs": {
        "location": [90, 160, 1],
        "scale": [90, 160, 2],
        "classes": [90, 160, 5],
        "hash": [90, 160, 16]
    },
    "batch_size": 32,
    "labels": {
        "location": ["x", "y"],
        "scale": ["width", "height"]
        "classes": ["pedestrian", "two-wheel", "small", "mid", "large"],
        "hash": {"type": "dhash", "size": 4}
    },
    "shuffle": true,
    "steps": 1000,
    "augmentation": "light",
    "preprocess": null
}
