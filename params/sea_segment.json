{
    "dataset": "singapore_maritime",
    "global": {
        "inputs": {
            "image": {
                "image": 1,
                "shape": [360, 640]
            }
        },
        "outputs": {
            "mask": {
                "mask": 1,
                "classes": ["sea"],
                "shape": [90, 160]
            }
        }
    },
    "batch_size": 32,
    "shuffle": true,
    "steps": 1000,
    "augmentation": "light",
    "preprocess": null
}
